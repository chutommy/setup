{
    "tsserver.enable": true,
    "tsserver.formatOnType": true,
    "tsserver.disableAutomaticTypeAcquisition": false,
    "prettier.endOfLine": "auto",
    "prettier.trailingComma": "all",
    "prettier.bracketSpacing": true,
    "prettier.useTabs": false,
  "json.enable": true,
  "json.trace.server": "verbose",
  "json.format.enable": true,
  "yaml.format.enable": true,
  "yaml.format.singleQuote": true,
  "yaml.validate": true,
  "yaml.completion": true,
  "yaml.hover": true,
  "yaml.format.bracketSpacing": true,
  "go.enable": true,
  "go.checkForUpdates": "disabled",
  "go.disable": {
    "completion": false,
    "diagnostics": false,
    "workspaceFolders": true
  },
  "go.goplsOptions": {
    "completeUnimported": true,
    "completionDocumentation": true,
    "deepCompletion": true,
    "expandWorkspaceToModule": true,
    "hoverKind": "FullDocumentation",
    "importShortcut": "Both",
    "linkTarget": "pkg.go.dev",
    "linksInHover": true,
    "matcher": "Fuzzy",
    "symbolStyle": "Full",
    "symbolMatcher": "Fuzzy",
    "tempModfile": true,
    "usePlaceholders": false
  },
  "vimlsp.diagnostic.enable": true,
  "vimlsp.suggest.fromRuntimepath": true,
  "python.linting.enabled": true,
  "python.jediEnabled": false,
  "python.autoComplete.addBrackets": true,
  "sh.enable": true,
  "sh.highlightParsingErrors": true,
  "sql.lintOnOpen": true,
  "sql.lintOnSave": true,
  "sql.lintOnChange": true,
  "sql.database": "postgresql",
  "docker.enable": true,
  "html.enable": true,
  "html.filetypes": ["html", "gohtml"],
  "html.autoClosingTags": true,
  "html.format.enable": true,
  "html.validate.html": true,
  "html.validate.styles": true,
  "html.validate.scripts": true,
  "html.suggest.ionic": true,
  "html.format.endWithNewline": true,
  "html.format.indentInnerHtml": true,
  "html.format.indentHandlebars": true,
  "markdownlint.onOpen": true,
  "markdownlint.onSave": true,
  "markdownlint.onChange": true,
  "markdownlint.config": {
    "default": false
  },
  "css.enable": true,
  "css.validate": true,
  "css.colorDecorators.enable": true,
  "css.completion.completePropertyWithSemicolon": true,
  "css.completion.triggerPropertyValueCompletion": true,
  "css.lint.duplicateProperties": "warning",
  "scss.completion.completePropertyWithSemicolon": true,
  "scss.completion.triggerPropertyValueCompletion": true,
  "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
  "emmet.showAbbreviationSuggestions": true,
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.optimizeStylesheetParsing": true,
  "highlight.colors.enable": true,
  "highlight.document.enable": true,
  "highlight.colorNames.enable": true,
  "pairs.enableCharacters": ["(", "[", "{", "<", "'", "\"", "`"],
      "pairs.enableBackspace": true
}
